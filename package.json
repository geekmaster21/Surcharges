  "displayName": "OrangeFox Recovery Downloads",
  "license": "AGPL-3.0-or-later",
  "homepage": "http://orangefox.download",
  "version": "21.5.11-0",
  "private": false,
  "funding": "https://opencollective.com/orangefox/donate",
  "scripts": {
    "pretty": "prettier --write .",
    "clean": "rimraf .next dist",
    "region": "node ./scripts/region.js",
    "dev:server": "cross-env NODE_ENV=development next dev -p 5000",
    "dev:server:open": "open-cli http://localhost:5000",
    "dev": "npm run region && concurrently -n dev,dev:open -c blue.bold,green.bold \"yarn dev:server\" \"yarn dev:server:open\"",
    "build": "yarn clean && npm run region && next build",
    "start": "next start -p 5001"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo '' && echo 'Running pre-commit code style fixes...' && echo '' && lint-staged --allow-empty"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.12.1",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "cookie": "^0.4.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.6",
    "js-cookie": "^2.2.1",
    "linkifyjs": "^2.1.9",
    "lodash.groupby": "^4.6.0",
    "lodash.isequal": "^4.5.0",
    "lodash.sortby": "^4.7.0",
    "lodash.startcase": "^4.4.0",
    "match-sorter": "^6.3.0",
    "next": "^11.0.1",
    "node-fetch": "^2.6.1",
    "node-sass": "5.x.x",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-easy-emoji": "^1.5.0",
    "react-intl": "^5.20.4"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^3.7.1",
    "@types/cookie": "^0.4.1",
    "@types/js-cookie": "^2.2.7",
    "@types/linkifyjs": "^2.1.4",
    "@types/lodash.groupby": "^4.6.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.sortby": "^4.7.6",
    "@types/lodash.startcase": "^4.4.6",
    "@types/match-sorter": "^6.0.0",
    "@types/node": "^16.3.2",
    "@types/node-fetch": "^2.5.11",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "concurrently": "^6.2.0",
    "critters": "^0.0.10",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.1",
    "open-cli": "^7.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "webpack": "^5.44.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
