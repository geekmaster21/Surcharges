{
  "name": "orangefox-recovery-download",
  "description": "This is the official OrangeFox Recovery repo of the download website",
  "author": {
    "name": "No Moderator",
    "email": "nomoderator@gmail.com"
  },
  "keywords": [
    "orangefox",
    "recovery",
    "android custom recovery"
  ],
  "contributors": [
    "Rahul Kurup",
    "No Moderator",
    "Yacha",
    "FD",
    "Community Translators"
